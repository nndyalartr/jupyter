    "class power:\n",
    "    def __init__(self,p,n):\n",
    "        self.number = p\n",
    "        self.exponent = n\n",
    "    def po(self):\n",
    "        powerof = (self.number**self.exponent)\n",
    "        print(\"power of the \", powerof)\n",
    "k = power(10,2)\n",
    "l = power(10,3)\n",
    "print(l.po())"
